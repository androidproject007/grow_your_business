<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:id="@+id/frame7"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        tools:context=".ui.activity.ImageCanvasActivity">
        <RelativeLayout
            android:id="@+id/sixteenth_frame_relative"
            android:layout_width="match_parent"

            android:layout_height="350dp">

            <RelativeLayout
                android:id="@+id/business_logo_relative"
                android:layout_width="60dp"
                android:layout_height="60dp"

                android:layout_marginLeft="@dimen/size_15dp"
                android:layout_marginTop="@dimen/dimen_5dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5dp">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/simple_curved_relative_5dp">
                        <ImageView
                            android:id="@+id/business_icon"
                            android:layout_width="800dp"
                            android:layout_height="80dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/brand_shaastra_logo" />
                    </RelativeLayout>

                </RelativeLayout>
                <ImageView
                    android:layout_width="@dimen/size_24"
                    android:layout_height="@dimen/size_24"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/blue_circuler_bg"
                    android:src="@drawable/_cancel"
                    android:visibility="gone" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/business_logo_relative1"
                android:layout_width="260dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:layout_alignParentRight="true">
                <RelativeLayout
                    android:id="@+id/layout1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:layout_alignParentLeft="true"
                    android:paddingLeft="10dp"
                    android:gravity="center_vertical">
                    <LinearLayout
                        android:id="@+id/test_name"
                        android:layout_width="wrap_content"

                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/business_company_details"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:text="growyourbusiness"></TextView>

                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@+id/layout1"
                    >

                    <ImageView
                        android:id="@+id/header_slash_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginTop="@dimen/dimen_5dp"
                        android:layout_marginBottom="@dimen/dimen_5dp"
                        android:src="@drawable/ic_icons8_line_24__2_"
                        android:tint="@color/black" />

                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/business_socialmedia_image_relative"
                    android:layout_width="110dp"
                    android:layout_height="70dp"
                    android:layout_alignParentRight="true"
                    android:padding="5dp"
                    >


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/asset_14social_media_frame35">

                    </LinearLayout>

                    <ImageView
                        android:layout_width="@dimen/dimen_10dp"
                        android:layout_height="@dimen/dimen_10dp"
                        android:background="@drawable/blue_circuler_bg"
                        android:src="@drawable/_cancel"
                        android:visibility="gone" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bottom_sixteenth_frame_main_relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_alignParentBottom="true">


                    <RelativeLayout
                        android:id="@+id/location_main_relative"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:background="@drawable/ic_asset_7round_border"
                        android:gravity="center">
                        <!-- <ImageView
                             android:id="@+id/footer_location_image"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:src="@drawable/ic_baseline_location_on_24" />-->
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center">

                            <LinearLayout
                                android:id="@+id/bottom_first_frame_first_linear"
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_marginLeft="20dp"
                                android:gravity="center_vertical|center_horizontal"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/mail_main_relative"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="20dp"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/footer_mail_icon"
                                        android:layout_width="15sp"
                                        android:layout_height="15sp"
                                        android:layout_marginLeft="10dp"
                                        android:src="@drawable/ic_mail"
                                        android:tint="@color/white" />

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <com.brandshaastra.utils.CustomTextView
                                            android:id="@+id/business_mail_details_text"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:ellipsize="end"
                                            android:gravity="center_horizontal"
                                            android:paddingLeft="@dimen/dimen_5dp"
                                            android:singleLine="true"
                                            android:text="For more enquiry email"
                                            android:textColor="@color/white"
                                            android:textSize="7dp" />

                                        <com.brandshaastra.utils.CustomTextView
                                            android:id="@+id/businessMailDetails"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:ellipsize="end"
                                            android:gravity="center_horizontal"
                                            android:paddingLeft="@dimen/dimen_5dp"
                                            android:singleLine="true"
                                            android:text="growyourbusiness@gmail.com"
                                            android:textColor="@color/white"
                                            android:textSize="7dp" />
                                    </LinearLayout>

                                </LinearLayout>

                                <Space
                                    android:layout_width="0dp"
                                    android:layout_height="1dp"

                                    android:layout_weight="1"></Space>

                                <ImageView
                                    android:id="@+id/footer_mail_image22"
                                    android:layout_width="10dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/asset_9round_border" />

                                <LinearLayout
                                    android:id="@+id/call_main_relative"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/footer_call_icon"
                                        android:layout_width="15sp"
                                        android:layout_height="15sp"
                                        android:src="@drawable/ic_asset_11message_adobe"
                                        android:tint="@color/white" />

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">


                                        <com.brandshaastra.utils.CustomTextView
                                            android:id="@+id/business_call_details_text"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:ellipsize="end"
                                            android:gravity="center_horizontal"
                                            android:paddingLeft="@dimen/dimen_5dp"
                                            android:singleLine="true"
                                            android:text="Call for registration"
                                            android:textColor="@color/white"
                                            android:textSize="7dp" />

                                        <com.brandshaastra.utils.CustomTextView
                                            android:id="@+id/businessCallDetails"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:ellipsize="end"
                                            android:gravity="center_horizontal"
                                            android:paddingLeft="@dimen/dimen_5dp"
                                            android:singleLine="true"
                                            android:text="0001 23445 88943"
                                            android:textColor="@color/white"
                                            android:textSize="7dp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <Space
                                    android:layout_width="0dp"
                                    android:layout_height="1dp"

                                    android:layout_weight="1"></Space>

                                <ImageView
                                    android:id="@+id/footer_mail_image21"
                                    android:layout_width="10dp"
                                    android:layout_height="30dp"
                                    android:src="@drawable/asset_9round_border" />

                                <LinearLayout
                                    android:id="@+id/website_main_relative"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/footer_website_icon"
                                        android:layout_width="15sp"
                                        android:layout_height="15sp"
                                        android:src="@drawable/ic_asset_12website_pointer"
                                        android:tint="@color/white" />

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">


                                        <com.brandshaastra.utils.CustomTextView
                                            android:id="@+id/business_website_details_text"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:ellipsize="end"
                                            android:gravity="center"
                                            android:paddingLeft="@dimen/dimen_5dp"
                                            android:singleLine="false"
                                            android:text="Visit our website for more"
                                            android:textColor="@color/white"
                                            android:textSize="7dp" />

                                        <com.brandshaastra.utils.CustomTextView
                                            android:id="@+id/businessWebsiteDetails"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:ellipsize="end"
                                            android:gravity="center_horizontal"
                                            android:paddingLeft="@dimen/dimen_5dp"
                                            android:singleLine="true"
                                            android:text="growyourbusiness.com"
                                            android:textColor="@color/white"
                                            android:textSize="7dp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <Space
                                    android:layout_width="0dp"
                                    android:layout_height="1dp"
                                    android:layout_weight="1"></Space>

                            </LinearLayout>

                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</layout>