<?xml version="1.0" encoding="utf-8"?><layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:id="@+id/frame10"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        tools:context=".ui.activity.ImageCanvasActivity">
        <RelativeLayout
            android:id="@+id/sixteenth_frame_relative"
            android:layout_width="match_parent"

            android:layout_height="350dp">
            <RelativeLayout
                android:id="@+id/business_logo_relative"
                android:layout_width="60dp"
                android:layout_height="60dp"

                android:layout_marginLeft="@dimen/size_15dp"
                android:layout_marginTop="@dimen/dimen_5dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5dp">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/simple_curved_relative_5dp">
                        <ImageView
                            android:id="@+id/business_icon"
                            android:layout_width="800dp"
                            android:layout_height="80dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/brand_shaastra_logo" />
                    </RelativeLayout>
                </RelativeLayout>
                <ImageView
                    android:layout_width="@dimen/size_24"
                    android:layout_height="@dimen/size_24"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/blue_circuler_bg"
                    android:src="@drawable/_cancel"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/business_logo_relative1"
                android:layout_width="170dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="3dp"
                android:background="@drawable/ic_top_footerline"
                android:backgroundTint="#000"
                android:gravity="center">

                <RelativeLayout
                    android:id="@+id/business_logo_relative1d"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/business_company_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:gravity="center"
                        android:text="growyourbusiness"

                        android:textColor="#032552"
                        android:textSize="8dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/business_socialmedia_image_relative"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/twitter_icon"
                        android:layout_width="15sp"
                        android:layout_height="15sp"
                        android:src="@drawable/ic_third_frame_twitter"
                        android:tint="@color/black" />

                    <ImageView
                        android:id="@+id/instagram_icon"
                        android:layout_width="15sp"
                        android:layout_height="15sp"
                        android:layout_toRightOf="@+id/twitter_icon"
                        android:src="@drawable/ic_third_frame_insta"
                        android:tint="@color/black" />

                    <ImageView
                        android:id="@+id/facebook_icon"
                        android:layout_width="15sp"
                        android:layout_height="15sp"
                        android:layout_toRightOf="@+id/instagram_icon"
                        android:src="@drawable/ic_third_frame_facebook"
                        android:tint="@color/black" />

                    <ImageView
                        android:id="@+id/whatsapp_icon"
                        android:layout_width="15sp"
                        android:layout_height="15sp"
                        android:layout_marginRight="5dp"
                        android:layout_toRightOf="@+id/facebook_icon"
                        android:src="@drawable/ic_third_frame_whatsapp"
                        android:tint="@color/black" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bottom_bg"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:background="@drawable/ic_frame_38_footer_background"
                android:forceHasOverlappingRendering="false">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true">


                    <RelativeLayout
                        android:id="@+id/footer_relative"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:alpha="0.8">

                        <LinearLayout
                            android:id="@+id/bottom_first_frame_first_linear"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_centerHorizontal="true"

                            android:gravity="center_vertical|center_horizontal"
                            android:orientation="horizontal">

                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"

                                android:layout_weight="1"></Space>

                            <LinearLayout
                                android:id="@+id/call_main_relative"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/footer_call_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="15dp"
                                    android:src="@drawable/ic_icons8_call_66"
                                    android:tint="@color/white" />

                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/businessCallDetails"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="+83479374983745"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />

                            </LinearLayout>

                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"

                                android:layout_weight="1"></Space>

                            <LinearLayout
                                android:id="@+id/mail_main_relative"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/footer_mail_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="15dp"
                                    android:src="@drawable/ic_mail"
                                    android:tint="@color/white" />

                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/businessMailDetails"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="growyourbusiness@gmail.com"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />

                            </LinearLayout>

                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"

                                android:layout_weight="1"></Space>

                            <LinearLayout
                                android:id="@+id/website_main_relative"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center">

                                <ImageView
                                    android:id="@+id/footer_website_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="15dp"
                                    android:src="@drawable/ic_website"
                                    android:tint="@color/white" />

                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/businessWebsiteDetails"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="growyourbusiness.com"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />
                            </LinearLayout>


                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"
                                android:layout_weight="1"></Space>
                        </LinearLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</layout>