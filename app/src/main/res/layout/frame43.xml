<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:id="@+id/frame15"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        tools:context=".ui.activity.ImageCanvasActivity">

        <RelativeLayout
            android:id="@+id/sixteenth_frame_relative"
            android:layout_width="match_parent"

            android:layout_height="350dp">

            <RelativeLayout
                android:id="@+id/business_logo_relative"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="@dimen/size_15dp"
                android:layout_marginTop="@dimen/dimen_5dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5dp">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/simple_curved_relative_5dp">

                        <ImageView
                            android:id="@+id/business_icon"
                            android:layout_width="800dp"
                            android:layout_height="80dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/brand_shaastra_logo" />
                    </RelativeLayout>

                </RelativeLayout>

                <ImageView
                    android:layout_width="@dimen/size_24"
                    android:layout_height="@dimen/size_24"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/blue_circuler_bg"
                    android:src="@drawable/_cancel"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/business_logo_relative1"
                android:layout_width="250dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:background="@drawable/asset_1frame43_background_header"
                android:gravity="center">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/twitter_icon1"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="@dimen/dimen_5dp"
                        android:src="@drawable/ic_icons8_line_24__2_"
                        android:tint="@color/black" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/business_logo_relative1d"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:background="@drawable/asset_18frame43_background_header"
                     android:gravity="center">

                    <TextView
                        android:id="@+id/website_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="14dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="2dp"
                        android:gravity="center"
                        android:text="websiteName"
                        android:textColor="@color/white"></TextView>

                    <ImageView
                        android:id="@+id/twitter_icon1d"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="@dimen/dimen_5dp"
                        android:layout_toRightOf="@+id/website_name"
                        android:src="@drawable/ic_icons8_line_24__2_"
                        android:tint="@color/white" />


                    <ImageView
                        android:id="@+id/twitter_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginTop="@dimen/dimen_5dp"
                        android:layout_marginRight="@dimen/dimen_5dp"
                        android:layout_marginBottom="@dimen/dimen_5dp"
                        android:layout_toRightOf="@+id/twitter_icon1d"
                        android:src="@drawable/ic_third_frame_twitter"
                        android:tint="@color/white" />

                    <ImageView
                        android:id="@+id/instagram_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginTop="@dimen/dimen_5dp"
                        android:layout_marginRight="@dimen/dimen_5dp"
                        android:layout_marginBottom="@dimen/dimen_5dp"
                        android:layout_toRightOf="@+id/twitter_icon"
                        android:src="@drawable/ic_third_frame_insta"
                        android:tint="@color/white" />

                    <ImageView
                        android:id="@+id/facebook_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginTop="@dimen/dimen_5dp"
                        android:layout_marginRight="@dimen/dimen_5dp"
                        android:layout_marginBottom="@dimen/dimen_5dp"
                        android:layout_toRightOf="@+id/instagram_icon"
                        android:src="@drawable/ic_third_frame_facebook"
                        android:tint="@color/white" />

                    <ImageView
                        android:id="@+id/whatsapp_icon"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginTop="@dimen/dimen_5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginBottom="@dimen/dimen_5dp"
                        android:layout_toRightOf="@+id/facebook_icon"
                        android:src="@drawable/ic_third_frame_whatsapp"
                        android:tint="@color/white" />


                    <!--    <ImageView
                            android:layout_width="@dimen/dimen_10dp"
                            android:layout_height="@dimen/dimen_10dp"
                            android:layout_alignRight="@+id/social_image_linear"
                            android:background="@drawable/blue_circuler_bg"
                            android:src="@drawable/_cancel"
                            android:visibility="gone" />-->
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bottom_sixteenth_frame_main_relative"
                android:layout_width="match_parent"
                android:background="@drawable/rectangle_1"
                android:layout_alignParentBottom="true"
                android:layout_height="30dp">

                <LinearLayout
                    android:id="@+id/bottom_first_frame_first_linear"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="20dp"
                    android:orientation="horizontal"
                    android:layout_alignParentBottom="true"
                    android:gravity="center_vertical|center_horizontal">
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1" ></Space>
                    <LinearLayout
                        android:id="@+id/website_main_relative"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/footer_website_image"
                            android:layout_width="10dp"
                            android:layout_height="10dp"

                            android:src="@drawable/ic_call"
                            android:tint="@color/white" />
                        <com.brandshaastra.utils.CustomTextView
                            android:id="@+id/business_website_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:paddingLeft="@dimen/dimen_5dp"
                            android:singleLine="true"
                            android:text="83488937489"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_9dp" />

                    </LinearLayout>
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1" ></Space>
                    <LinearLayout
                        android:id="@+id/mail_main_relative"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/footer_mail_image"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/ic_website"
                            android:tint="@color/white"/>
                        <com.brandshaastra.utils.CustomTextView
                            android:id="@+id/business_mail_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:paddingLeft="@dimen/dimen_5dp"
                            android:singleLine="true"
                            android:text="website.com"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_9dp" />

                    </LinearLayout>
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"

                        android:layout_weight="1" ></Space>
                    <LinearLayout
                        android:id="@+id/call_main_relative"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center">
                        <ImageView
                            android:id="@+id/footer_call_image"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/ic_eight_frame_mail__3_"
                            android:tint="@color/white" />
                        <com.brandshaastra.utils.CustomTextView
                            android:id="@+id/business_call_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:paddingLeft="@dimen/dimen_5dp"
                            android:singleLine="true"
                            android:text="Address Email related"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_9dp" />
                    </LinearLayout>
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1" ></Space>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</layout>