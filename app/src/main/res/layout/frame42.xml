<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:id="@+id/frame14"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        tools:context=".ui.activity.ImageCanvasActivity">
        <RelativeLayout
            android:id="@+id/sixteenth_frame_relative"
            android:layout_width="match_parent"

            android:layout_height="350dp">
            <RelativeLayout
                android:id="@+id/business_logo_relative"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="@dimen/size_15dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5dp">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/simple_curved_relative_5dp">
                        <ImageView
                            android:id="@+id/business_icon"
                            android:layout_width="800dp"
                            android:layout_height="80dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/brand_shaastra_logo" />
                    </RelativeLayout>
                </RelativeLayout>
                <ImageView
                    android:layout_width="@dimen/size_24"
                    android:layout_height="@dimen/size_24"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/blue_circuler_bg"
                    android:src="@drawable/_cancel"
                    android:visibility="gone" />
            </RelativeLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/business_logo_relative1"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_alignParentLeft="true"
                android:layout_marginEnd="0dp"
                android:layout_toStartOf="@+id/business_logo_relative"
             >

                <RelativeLayout
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    tools:ignore="MissingConstraints">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/twitter_icon1"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:src="@drawable/ic_icons8_line_24__2_"
                            android:tint="@color/black" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/business_logo_relative1d"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:background="@drawable/asset_7frame42_header"
                        android:paddingTop="2dp">

                        <TextView
                            android:id="@+id/business_company_details"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="Grow Your Business"
                            android:textColor="@color/white"
                            android:textSize="8dp"></TextView>
            <RelativeLayout
                android:id="@+id/business_socialmedia_image_relative"
                android:layout_width="wrap_content"
                android:layout_toRightOf="@+id/business_company_details"
                android:layout_height="wrap_content"
                >
                        <ImageView
                            android:id="@+id/header_slash_icon"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_toRightOf="@+id/business_company_details"
                            android:src="@drawable/ic_icons8_line_24__2_"
                            android:tint="@color/white" />

                        <ImageView
                            android:id="@+id/twitter_icon"
                            android:layout_width="10dp"
                            android:layout_height="10dp"

                            android:layout_toRightOf="@+id/header_slash_icon"
                            android:src="@drawable/ic_third_frame_twitter"
                            android:tint="@color/white" />

                        <ImageView
                            android:id="@+id/instagram_icon"
                            android:layout_width="10dp"
                            android:layout_height="10dp"

                            android:layout_toRightOf="@+id/twitter_icon"
                            android:src="@drawable/ic_third_frame_insta"
                            android:tint="@color/white" />

                        <ImageView
                            android:id="@+id/facebook_icon"
                            android:layout_width="10dp"
                            android:layout_height="10dp"

                            android:layout_toRightOf="@+id/instagram_icon"
                            android:src="@drawable/ic_third_frame_facebook"
                            android:tint="@color/white" />

                        <ImageView
                            android:id="@+id/whatsapp_icon"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_toRightOf="@+id/facebook_icon"
                            android:src="@drawable/ic_third_frame_whatsapp"
                            android:tint="@color/white" />
                        <!--    <ImageView
                            android:layout_width="@dimen/dimen_10dp"
                            android:layout_height="@dimen/dimen_10dp"
                            android:layout_alignRight="@+id/social_image_linear"
                            android:background="@drawable/blue_circuler_bg"
                            android:src="@drawable/_cancel"
                            android:visibility="gone" />-->
            </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <RelativeLayout
                android:id="@+id/bottom_sixteenth_frame_main_relative"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true">
                    <!-- <ImageView
                             android:id="@+id/footer_location_image"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:src="@drawable/ic_baseline_location_on_24" />-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/call_main_relative"
                            android:layout_width="200dp"
                            android:layout_height="15dp"
                            android:background="@drawable/asset_16frame42_background_footer1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/icon_layout_footer1"
                                android:layout_width="25dp"
                                android:layout_height="40dp"
                                android:background="@drawable/asset_12frame42_footer_icon">

                                <ImageView
                                    android:id="@+id/footer_call_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="5dp"
                                    android:src="@drawable/asset_10frame42_call_icon" />
                            </LinearLayout>

                            <com.brandshaastra.utils.CustomTextView
                                android:id="@+id/business_call_details"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/icon_layout_footer1"
                                android:paddingLeft="@dimen/dimen_5dp"
                                android:singleLine="false"
                                android:text="232344546"
                                android:textColor="@color/white"
                                android:textSize="8dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/mail_main_relative12"
                            android:layout_width="wrap_content"
                            android:layout_height="15dp"
                            android:background="@drawable/asset_16frame42_background_footer1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/icon_layout_footer"
                                android:layout_width="25dp"
                                android:layout_height="wrap_content"
                                android:background="@drawable/asset_12frame42_footer_icon">

                                <ImageView
                                    android:id="@+id/footer_website_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_centerVertical="true"

                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="5dp"
                                    android:src="@drawable/asset_7frame42_website_icon" />
                            </LinearLayout>

                            <com.brandshaastra.utils.CustomTextView
                                android:id="@+id/business_website_details"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/icon_layout_footer"
                                android:paddingLeft="@dimen/dimen_5dp"
                                android:singleLine="false"
                                android:text="WWW.testWebsite.com"
                                android:textColor="@color/white"
                                android:textSize="8dp" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/mail_main_relative123"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/mail_main_relative112"
                            android:layout_width="200dp"
                            android:layout_height="15dp"
                            android:background="@drawable/asset_16frame42_background_footer1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/icon_layout_footer14"
                                android:layout_width="25dp"
                                android:layout_height="40dp"
                                android:background="@drawable/asset_12frame42_footer_icon">

                                <ImageView
                                    android:id="@+id/footer_mail_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="5dp"
                                    android:src="@drawable/asset_9frame42_email_icon" />
                            </LinearLayout>

                            <com.brandshaastra.utils.CustomTextView
                                android:id="@+id/business_mail_details"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/icon_layout_footer14"
                                android:paddingLeft="@dimen/dimen_5dp"
                                android:singleLine="false"
                                android:text="Email@gmail.com"
                                android:textColor="@color/white"
                                android:textSize="8dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/mail_main_relative1234"
                            android:layout_width="wrap_content"
                            android:layout_height="15dp"
                            android:background="@drawable/asset_16frame42_background_footer1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/icon_layout_footer4"
                                android:layout_width="25dp"
                                android:layout_height="20dp"
                                android:background="@drawable/asset_12frame42_footer_icon">

                                <ImageView
                                    android:id="@+id/footer_location_icon"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:layout_centerVertical="true"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="5dp"
                                    android:src="@drawable/asset_8frame42_location_icon" />
                            </LinearLayout>

                            <com.brandshaastra.utils.CustomTextView
                                android:id="@+id/business_location_details"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/icon_layout_footer4"
                                android:paddingLeft="@dimen/dimen_5dp"
                                android:singleLine="false"
                                android:text="Location | Surat | Mumbai"
                                android:textColor="@color/white"
                                android:textSize="8dp" />
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="8dp"
                        android:gravity="center">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="Service | Service | Service | Service | Service | Service "
                            android:textColor="@color/black"
                            android:textSize="8dp"></TextView>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</layout>