<?xml version="1.0" encoding="utf-8"?><layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:id="@+id/frame12"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        tools:context=".ui.activity.ImageCanvasActivity">
        <RelativeLayout
            android:id="@+id/sixteenth_frame_relative"
            android:layout_width="match_parent"

            android:layout_height="350dp">

            <RelativeLayout
                android:id="@+id/business_logo_relative"
                android:layout_width="30dp"
                android:layout_height="30dp"

                android:layout_marginLeft="@dimen/size_15dp"
                android:layout_marginTop="@dimen/dimen_5dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5dp">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/simple_curved_relative_5dp">
                        <ImageView
                            android:id="@+id/business_icon"
                            android:layout_width="800dp"
                            android:layout_height="80dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/brand_shaastra_logo" />
                    </RelativeLayout>

                </RelativeLayout>
                <ImageView
                    android:layout_width="@dimen/size_24"
                    android:layout_height="@dimen/size_24"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/blue_circuler_bg"
                    android:src="@drawable/_cancel"
                    android:visibility="gone" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/business_logo_relative1"
                android:layout_width="230dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:layout_alignParentRight="true">
                <RelativeLayout
                    android:id="@+id/layout1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:layout_alignParentLeft="true"
                    android:paddingLeft="10dp"
                    android:gravity="center_vertical">
                    <LinearLayout
                        android:id="@+id/test_name"
                        android:layout_width="wrap_content"

                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textColor="@color/black"
                            android:text="websiteName"></TextView>

                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:paddingRight="10dp"
                    >
                    <ImageView
                        android:id="@+id/twitter_icon1"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="@dimen/dimen_5dp"
                        android:src="@drawable/ic_icons8_line_24__2_"
                        android:tint="@color/black" />

                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/business_socialmedia_image_relative"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:background="@color/white">


                    <LinearLayout
                        android:id="@+id/social_image_linear"
                        android:layout_width="wrap_content"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/twitter_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:layout_marginRight="@dimen/dimen_5dp"
                            android:layout_marginBottom="@dimen/dimen_5dp"
                            android:src="@drawable/ic_third_frame_twitter"
                            android:tint="@color/black" />

                        <ImageView
                            android:id="@+id/instagram_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:layout_marginRight="@dimen/dimen_5dp"
                            android:layout_marginBottom="@dimen/dimen_5dp"
                            android:src="@drawable/ic_third_frame_insta"
                            android:tint="@color/black" />

                        <ImageView
                            android:id="@+id/facebook_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:layout_marginRight="@dimen/dimen_5dp"
                            android:layout_marginBottom="@dimen/dimen_5dp"
                            android:src="@drawable/ic_third_frame_facebook"
                            android:tint="@color/black" />

                        <ImageView
                            android:id="@+id/whatsapp_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="@dimen/dimen_5dp"
                            android:layout_marginBottom="@dimen/dimen_5dp"
                            android:src="@drawable/ic_third_frame_whatsapp"
                            android:tint="@color/black" />


                    </LinearLayout>

                    <ImageView
                        android:layout_width="@dimen/dimen_10dp"
                        android:layout_height="@dimen/dimen_10dp"
                        android:layout_alignRight="@+id/social_image_linear"
                        android:background="@drawable/blue_circuler_bg"
                        android:src="@drawable/_cancel"
                        android:visibility="gone" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bottom_sixteenth_frame_main_relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true">
                    <RelativeLayout
                        android:id="@+id/location_main_relative"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:backgroundTint="#6B6C6C"
                        android:background="@drawable/desh_line"
                        android:padding="2dp">
                        <!-- <ImageView
                             android:id="@+id/footer_location_image"
                             android:layout_width="wrap_content"
                             android:layout_height="wrap_content"
                             android:src="@drawable/ic_baseline_location_on_24" />-->
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="#6B6C6C"

                            android:gravity="center">
                            <ImageView
                                android:id="@+id/footer_location_image"
                                android:layout_width="10dp"
                                android:layout_height="10dp"

                                android:src="@drawable/ic_website"
                                android:tint="@color/white"
                                android:visibility="invisible"/>
                            <com.brandshaastra.utils.CustomTextView
                                android:id="@+id/business_location_details"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="10dp"
                                android:ellipsize="end"
                                android:layout_marginBottom="5dp"
                                android:layout_marginRight="10dp"
                                android:singleLine="true"
                                android:text="Address"
                                android:paddingTop="-20dp"
                                android:textColor="@color/white"
                                android:textSize="10dp" />
                        </RelativeLayout>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/footer_relative"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"

                        android:background="#171C30">
                        <TextView
                            android:id="@+id/frame19_view1"
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="@color/black" />
                        <LinearLayout
                            android:id="@+id/bottom_first_frame_first_linear"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:orientation="horizontal"
                            android:background="#171C30"
                            android:gravity="center_vertical|center_horizontal">


                            <LinearLayout
                                android:id="@+id/website_main_relative"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">
                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/Email_icon"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="Email:"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />
                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/business_website_details"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="TestEmail@gmail.com"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />

                            </LinearLayout>

                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"

                                android:layout_weight="1" ></Space>

                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"

                                android:layout_weight="1" ></Space>
                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"

                                android:layout_weight="1" ></Space>

                            <LinearLayout
                                android:id="@+id/call_main_relative1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center">
                                <ImageView
                                    android:id="@+id/footer_call_image1"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:src="@drawable/call_black"
                                    android:tint="@color/white" />
                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/business_call_details1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="Ahmedabad"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />
                            </LinearLayout>
                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"
                                android:layout_weight=".5" ></Space>

                            <LinearLayout
                                android:id="@+id/call_main_relative"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:gravity="center">

                                <ImageView
                                    android:id="@+id/footer_call_image"
                                    android:layout_width="10dp"
                                    android:layout_height="10dp"
                                    android:src="@drawable/ic_website"
                                    android:tint="@color/white" />
                                <com.brandshaastra.utils.CustomTextView
                                    android:id="@+id/business_call_details"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:paddingLeft="@dimen/dimen_5dp"
                                    android:singleLine="true"
                                    android:text="www.testWesite.com"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/size_9dp" />
                            </LinearLayout>
                            <Space
                                android:layout_width="0dp"
                                android:layout_height="1dp"
                                android:layout_weight=".5" ></Space>
                        </LinearLayout>
                    </RelativeLayout>

                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</layout>